<Window
	x:Class="Hammerfrost.CardStudio.Program.MainWindow"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	xmlns:local="clr-namespace:Hammerfrost.CardStudio.Program"
	xmlns:resources="clr-namespace:Hammerfrost.CardStudio.Program.Resources"
	mc:Ignorable="d"
	Title="Hammerfrost Card Studio 0.0.0"
	Width="1440" Height="900"
	>

	<Grid>
		<StackPanel
			Orientation="Vertical"
			VerticalAlignment="Center"
			MaxWidth="200"
			>
			<Button
				Margin="0,0,0,20"
				FontSize="24"
				>
				New Project
			</Button>
			<Button
				Margin="0,0,0,20"
				FontSize="24"
				>
				Open Project
			</Button>
		</StackPanel>
	</Grid>
</Window>
