<Project>

	<!-- directories -->
	<PropertyGroup>
		<BuildDir>_build\</BuildDir>
		<LogDir>_log\</LogDir>
		<OutputDir>_output\</OutputDir>
		<PreBuildOutputDir>PreBuildOutput\</PreBuildOutputDir>
		<GitIgnorePreBuildOutputDir>$(PreBuildOutputDir)GitIgnore\</GitIgnorePreBuildOutputDir>
		<TestDir>_test\</TestDir>
		<TestInputDir>$(TestDir)_input\</TestInputDir>
		<TestLogDir>$(TestDir)$(LogDir)</TestLogDir>
		<TestOutputDir>$(TestDir)$(OutputDir)</TestOutputDir>
	</PropertyGroup>

	<!-- output paths -->
	<PropertyGroup>
		<AppendTargetFrameworkToOutputPath>false</AppendTargetFrameworkToOutputPath>
		<AppendRuntimeIdentifierToOutputPath>false</AppendRuntimeIdentifierToOutputPath>
		<BaseIntermediateOutputPath>$(SolutionDir)$(BuildDir)$(MSBuildProjectName)\obj\</BaseIntermediateOutputPath>
		<IntermediateOutputPath>$(BaseIntermediateOutputPath)$(Configuration)\</IntermediateOutputPath>
		<BaseOutputPath>$(SolutionDir)$(BuildDir)$(MSBuildProjectName)\bin\</BaseOutputPath>
		<OutputPath>$(BaseOutputPath)$(Configuration)\</OutputPath>
	</PropertyGroup>

	<!-- project properties -->
	<PropertyGroup>
		<CompanyName>Hammerfrost</CompanyName>
		<ProjectName>CardStudio</ProjectName>
		<ProjectVersion>0.0.0</ProjectVersion>
	</PropertyGroup>

	<!-- strings -->
	<PropertyGroup>
		<AutoGeneratedFileNotice>//AUTO-GENERATED FILE, DO NOT EDIT; ANY CHANGES WILL BE OVERWRITTEN DURING BUILD</AutoGeneratedFileNotice>
		<DoNotCommitFileNotice>//DO NOT COMMIT THIS FILE, IT CONTAINS POTENTIALLY IDENTIFYING INFORMATION</DoNotCommitFileNotice>
	</PropertyGroup>

</Project>
